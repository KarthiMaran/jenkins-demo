    agent any 

    stages {
        stage('Build') {
            steps {
                echo 'Building my app...'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying to the server!'
            }
        }
    }
}
